<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="-262dp"
    tools:layout_editor_absoluteY="-135dp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Select a new Logo"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.217" />

    <ImageButton
        android:id="@+id/ottawaLogo"
        android:layout_width="116dp"
        android:layout_height="118dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toStartOf="@+id/starLogo"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/ic_logo_01" />

    <ImageButton
        android:id="@+id/starLogo"
        android:layout_width="116dp"
        android:layout_height="118dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/ic_logo_02" />

    <ImageButton
        android:id="@+id/bmvLogo"
        android:layout_width="116dp"
        android:layout_height="118dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="38dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/starLogo"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/ic_logo_03" />

    <ImageButton
        android:id="@+id/fcbLogo"
        android:layout_width="116dp"
        android:layout_height="118dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/canadaLogo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ottawaLogo"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_logo_04" />

    <ImageButton
        android:id="@+id/randomLogo"
        android:layout_width="116dp"
        android:layout_height="118dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/canadaLogo"
        app:layout_constraintTop_toBottomOf="@+id/bmvLogo"
        app:layout_constraintVertical_bias="0.051"
        app:srcCompat="@drawable/ic_logo_00" />

    <ImageButton
        android:id="@+id/canadaLogo"
        android:layout_width="116dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/starLogo"
        app:layout_constraintStart_toStartOf="@+id/starLogo"
        app:layout_constraintTop_toBottomOf="@+id/starLogo"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_logo_05" />

</androidx.constraintlayout.widget.ConstraintLayout>